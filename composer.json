{
  "name": "thomasvargiu/proxy-manager-module",
  "description": "A ZF module which allows to configure and use Ocramius/ProxyManager via ServiceManager",
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/thomasvargiu/ProxyManagerModule",
  "keywords": [
    "zf2",
    "zf3",
    "zend framework",
    "proxy",
    "proxy pattern",
    "service proxies",
    "lazy loading",
    "aop"
  ],
  "authors": [
    {
      "name": "Thomas Vargiu",
      "email": "tvargiu@gmail.com",
      "homepage": "http://thomasvargiu.com/"
    }
  ],
  "require": {
    "php": "~7.0",
    "ocramius/proxy-manager": "^2.0",
    "zendframework/zend-servicemanager": "^2.7 || ^3.0",
    "zendframework/zend-modulemanager": "^2.7",
    "container-interop/container-interop": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.2.2",
    "squizlabs/php_codesniffer": "^2.6",
    "phpmd/phpmd" : "^2.4",
    "fabpot/php-cs-fixer": "^1.11"
  },
  "autoload": {
    "psr-4": {
      "ProxyManagerModule\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ProxyManagerModuleTest\\": "tests/unit/"
    }
  }
}
