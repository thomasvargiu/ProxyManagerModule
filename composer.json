{
  "name": "thomasvargiu/proxy-manager-module",
  "description": "A ZF2 which allows to configure and use Ocramius/ProxyManager via ServiceManager",
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/thomasvargiu/ProxyManagerModule",
  "keywords": [
    "zf2",
    "zend framework",
    "proxy",
    "proxy pattern",
    "service proxies",
    "lazy loading",
    "aop"
  ],
  "authors": [
    {
      "name": "Thomas Vargiu",
      "email": "tvargiu@gmail.com",
      "homepage": "http://thomasvargiu.com/"
    }
  ],
  "require": {
    "php": ">=5.4",
    "ocramius/proxy-manager": "~0.5",
    "zendframework/zend-stdlib": "~2.2",
    "zendframework/zend-servicemanager": "~2.2",
    "zendframework/zend-modulemanager": "~2.2"
  },
  "require-dev": {
    "phpunit/phpunit": "@stable",
    "squizlabs/php_codesniffer": "1.5.*"
  },
  "autoload": {
    "psr-4": {
      "ProxyManagerModule\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ProxyManagerModuleTest\\": "tests/"
    }
  }
}
